<?php 
class Sala_immersiva{
    private $numero_posti;
    private $numero_sala;
    private $effetti_speciali;

    public function __construct($_num_posti,$_num_sala,$_effetti_speciali){
        $this->num_posti = $_num_posti;
        $this->num_sala = $_num_sala;
        $this->effetti_speciali = $_effetti_speciali;
    }
}
?>